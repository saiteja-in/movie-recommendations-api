{
  "name": "movie-recommendation-api",
  "version": "1.0.0",
  "description": "AI-powered movie recommendation system API",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/app.js",
    "dev": "ts-node-dev --respawn --transpile-only src/app.ts",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "seed": "ts-node src/scripts/seedDatabase.ts",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "prisma db seed",
    "db:studio": "prisma studio"
  },
  "keywords": ["movie", "recommendation", "ai", "api", "typescript"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "zod": "^3.22.4",
    "openai": "^4.28.4",
    "uuid": "^9.0.1",
    "dotenv": "^16.4.5",
    "express-rate-limit": "^7.1.5",
    "node-cache": "^5.1.2",
    "@prisma/client": "^5.7.1",
    "axios": "^1.6.5",
    "prom-client": "^15.1.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/morgan": "^1.9.9",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/bcryptjs": "^2.4.6",
    "@types/uuid": "^9.0.7",
    "@types/node": "^20.11.19",
    "typescript": "^5.3.3",
    "ts-node-dev": "^2.0.0",
    "ts-node": "^10.9.1",
    "@types/dotenv": "^8.2.0",
    "@types/express-rate-limit": "^6.0.0",
    "@types/axios": "^0.14.0",
    "prisma": "^5.7.1",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0"
  }
}